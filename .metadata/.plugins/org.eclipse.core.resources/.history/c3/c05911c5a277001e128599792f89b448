import java.util.Scanner;

public class Q38 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner kk= new Scanner(System.in);
    	System.out.print("enter a number: ");
    	int n= kk.nextInt();
    	int sum=1;
    	
		do{
			System.out.println(sum);
			sum= sum+n;
			n++;
		}while(n<=10);
	kk.close();	
	}

}
