import java.util.Scanner;

public class Q32 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner kb= new Scanner(System.in);
		System.out.println("Enter a num : ");
		int num=kb.nextInt();
		int fibonacci =0;
		int i= 0;
		
		while(i<num){
			i++;
			fibonacci=num+i;
			System.out.println(fibonacci);
		}
		
	}

}
