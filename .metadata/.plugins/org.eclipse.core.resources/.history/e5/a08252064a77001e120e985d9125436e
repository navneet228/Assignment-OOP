import java.util.Scanner;

public class Q34 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner kb= new Scanner(System.in);
		System.out.print("Enter a number : ");
		int num=kb.nextInt();
		int i=0;
		while( i<num) {
			i++;
			int mpl=i*num;
			System.out.print(mpl);
		}
		System.out.println();
	}

}
