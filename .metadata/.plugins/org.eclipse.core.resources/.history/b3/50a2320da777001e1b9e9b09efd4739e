import java.util.Scanner;

public class Q40 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner kk= new Scanner(System.in);
    	System.out.print("enter a number: ");
    	int n= kk.nextInt();
    	int i=0;
    	int fibo= 0;
    	
    	do {
    		i++;
    		fibo=i+n;
    	}while(i<10);
    	System.out.print(fibo);

	}

}
